                  <pre class="line-pre"><div class="line" id="file-getstyleclass-LC1">function getStyleClass(className) {
</div><div class="line" id="file-getstyleclass-LC2">    for (var s = 0; s &lt; document.styleSheets.length; s++) {
</div><div class="line" id="file-getstyleclass-LC3">        if(document.styleSheets[s].rules) {
</div><div class="line" id="file-getstyleclass-LC4">            for (var r = 0; r &lt; document.styleSheets[s].rules.length; r++) {
</div><div class="line" id="file-getstyleclass-LC5">                if (document.styleSheets[s].rules[r].selectorText == &#39;.&#39; + className) {
</div><div class="line" id="file-getstyleclass-LC6">                    return document.styleSheets[s].rules[r];
</div><div class="line" id="file-getstyleclass-LC7">                }
</div><div class="line" id="file-getstyleclass-LC8">            }
</div><div class="line" id="file-getstyleclass-LC9">        } else if(document.styleSheets[s].cssRules) {
</div><div class="line" id="file-getstyleclass-LC10">            for (var r = 0; r &lt; document.styleSheets[s].cssRules.length; r++) {
</div><div class="line" id="file-getstyleclass-LC11">                if (document.styleSheets[s].cssRules[r].selectorText == &#39;.&#39; + className)
